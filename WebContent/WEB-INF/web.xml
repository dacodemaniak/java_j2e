<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>MyResa</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>com.webprojet.web.ServletLogin</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Logout</servlet-name>
    <servlet-class>com.webprojet.web.ServletLogout</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Logout</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AddSpectacle</servlet-name>
    <servlet-class>com.webprojet.web.ServletAddSpectacle</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddSpectacle</servlet-name>
    <url-pattern>/addSpectacle</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AddReservation</servlet-name>
    <servlet-class>com.webprojet.web.ServletAddReservation</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddReservation</servlet-name>
    <url-pattern>/addReservation</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>UpdSpectacle</servlet-name>
    <servlet-class>com.webprojet.web.ServletUpdSpectacle</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UpdSpectacle</servlet-name>
    <url-pattern>/updSpectacle</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>GetSpectacle</servlet-name>
  	<servlet-class>com.webprojet.web.ServletGetSpectacle</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>GetSpectacle</servlet-name>
  	<url-pattern>/getSpectacle</url-pattern>
  </servlet-mapping>
</web-app>